<%= render :partial => "forms/#{form.status}_listing", :locals => {:form => form, :pre_text => "(#{Thread.current['user'].is_admin? ? form.status : form.status.as_status.word('lowercase short singular not-mine')
}" + (form.assigned_to.to_i > 0 && !form.assigned.nil? && Thread.current['user'].is_store_admin? ? " - assigned to #{form.assigned.friendly_name}" : '') + ")"} unless form.status == 'all' %>
