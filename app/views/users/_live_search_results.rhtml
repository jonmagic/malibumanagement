<% for user in @results %>
  <tr<%= " class='store_admin'" if user.is_store_admin? %>>
    <td><%= user.friendly_name %></td>
    <td><%= user.username %></td>
    <td><%= link_to 'Show', user_path(:action=> 'show', :id => user.id) %></td>
    <td><%= link_to 'Destroy', user_path(:id => user.id), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
